<br><br><%= form_for @user do |f| %>
<h1>Edit Profile</h1>
<%= f.label :name %>
<%= f.text_field :name %> <br>
<%= f.label :password %>
<%= f.password_field :password %> <br>
<%= f.label :email %>
<%= f.text_field :email %> <br>
<%= f.label :location %>
<%= f.number_field :location %> <br>
<%= f.submit %>
<% end %>
